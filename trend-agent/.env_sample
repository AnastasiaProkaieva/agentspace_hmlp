# =============================================================================
# AGENT CONFIGURATION
# =============================================================================
# =============================================================================
# REQUIRED: AgentSpace / ADK Configuration
# =============================================================================
# Your Google Cloud Project ID (required)
GOOGLE_CLOUD_PROJECT=""
# Your Google Cloud Project Number (required for agent space creation)
GOOGLE_CLOUD_PROJECT_NUMBER=""
GOOGLE_CLOUD_LOCATION=""
GOOGLE_CLOUD_LOCATION_AS=""
# Cloud Storage bucket for staging (required)
STAGING_BUCKET=""
# AgentSpace Authorization ID (set after running create_authorization.sh)
AUTH_ID="report-prep-auth"
# Display name for your agent
AGENT_DISPLAY_NAME="Report Research Agent"
# Backend URL where the Human Evaluator UI is hosted (required)
REVIEW_APP_BASE_URL="YOUR_BACKEND_URL"
# Emnail address of the reviewer (required)
REVIEWER_EMAIL=""
# If you want to call models from Vertex AI, set this to TRUE
GOOGLE_GENAI_USE_VERTEXAI=TRUE
# =============================================================================
# REQUIRED: OAuth 2.0 Credentials (for AgentSpace)
# =============================================================================
# Get these from Google Cloud Console > APIs & Services > Credentials
OAUTH_CLIENT_ID=""
OAUTH_CLIENT_SECRET=""
# =============================================================================
# OAuth Scopes (used by create_authorization.sh)
# =============================================================================
# Space-separated list of required OAuth scopes
#SCOPES="https://www.googleapis.com/auth/userinfo.email https://www.googleapis.com/auth/gmail.send https://www.googleapis.com/auth/drive.file"
# =============================================================================
# REQUIRED: AgentSpace Configuration
# =============================================================================
# Your AgentSpace application identifier
AS_APP_ID=""
# Assistant ID (usually "default_assistant")
ASSISTANT_ID="default_assistant"
# Unique agent name
AGENT_NAME="market_report_agent"
# Agent description
AGENT_DESCRIPTION="Prepares market reports based on user queries."
# Tool description
AGENT_TOOL_DESCRIPTION="Gathers information from the Google Search and generate a report based on the user ask. In case if any competitor is mentioned or if the report size exceeds 500 chars the verification is send to the human. "
# Reasoning Engine resource name (set after agent deployment)
REASONING_ENGINE=""
# =============================================================================
# AI Models (optional - defaults to gemini-2.5-flash)
# =============================================================================
ROOT_AGENT_MODEL="gemini-2.5-flash"
SUB_AGENT_MODEL="gemini-2.5-flash"
